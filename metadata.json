{
  "name": "mybuilder-kongfig",
  "version": "0.1.2",
  "author": "MyBuilder Limited",
  "summary": "Install kongfig which is a tool for declaratively configuring Kong",
  "license": "Apache-2.0",
  "source": "git@github.com:mybuilder/puppet-kongfig.git",
  "project_page": "https://github.com/mybuilder/puppet-kongfig",
  "issues_url": "https://github.com/mybuilder/puppet-kongfig/issues",
  "description": "This will install kongfig to enable you to declaratively configure your Kong instance",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": ["7", "8"]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["14.04"]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.2.x"
    },
    {
      "name": "puppet",
      "version_requirement": ">=2.7.20 <5.0.0"
    }
  ],
  "dependencies": [
    {"name":"puppet-nodejs","version_requirement":">=1.1.0"},
    {"name":"puppetlabs/stdlib","version_requirement":">=4.0.0 < 5.0.0"}
  ]
}
